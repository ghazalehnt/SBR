{
  "dataset": {
    "name": "CGR",
    "dataset_path": "<DATA_ROOT_PATH>/GR_read_5-folds/split_1/",
    "binary_interactions": true,
    "relevance_level": 1,
    "train_batch_size": 1024,
    "eval_batch_size": 32,
    "user_text": [],
    "item_text": [],
    "training_neg_sampling_strategy": "random",
